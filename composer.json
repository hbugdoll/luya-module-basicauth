{
    "name" : "luyadev/luya-module-basicauth",
    "description" : "Adds the typical htaccess auth dialog before each request. This module is made as angular does have problems with basic htaccess authentifications in the administration area.",
    "type" : "luya-module",
    "keywords" : ["php", "yii2", "luya", "basicauth", "htaccess", "auth", "luya-module"],
    "license": "MIT",
    "homepage" : "http://luya.io",
    "authors" : [
        {
            "name" : "Basil",
            "email" : "git@nadar.io",
            "homepage" : "https://github.com/nadar"
        },
        {
            "name" : "Mikhail Chemezov",
            "email" : "michlenanosoft@gmail.com",
            "homepage" : "https://github.com/chemezov"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "luya\\basicauth\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "luya\\basicauth\\tests\\" : "tests/"
        }
    },
    "require-dev": {
        "unglue/client" : "^1.0",
        "luyadev/luya-testsuite": "^2.0"
    },
    "config": {
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
